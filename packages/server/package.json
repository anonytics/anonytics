{
  "name": "@anonytics/server",
  "version": "0.1.0",
  "description": "The Anonytics server that securely retrieves and stores anonymous user data",
  "repository": "https://github.com/anonytics/anonytics-javascript",
  "author": "Jeppe Reinhold <jeppereinhold@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "next",
    "start:prod": "next start",
    "build": "next build",
    "test": "yarn test:once --watch",
    "test:once": "cd ../../ && jest --projects packages/server",
    "test:branch": "yarn test:once --changedSince=master",
    "knex": "yarn ts-node --project ./tsconfig.knex.json node_modules/.bin/knex",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"./{src/*,*}.{js,jsx,ts,tsx,json,md,yml}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "apollo-server-micro": "2.6.7",
    "graphql": "14.4.2",
    "isomorphic-unfetch": "3.0.0",
    "knex": "^0.19.1",
    "next": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "ts-node": "^8.3.0"
  }
}
